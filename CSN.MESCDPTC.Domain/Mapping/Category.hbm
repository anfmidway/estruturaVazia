<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
 assembly="CSN.MESCDPTC.Domain" namespace="CSN.MESCDPTC.Domain.Mapping">
	<class name="Category">
		<cache usage="nonstrict-read-write"/>
		<id name="Id">
			<generator class="identity"/>
		</id>
		<property name="Name"/>
		<bag name="Products">
			<cache usage="nonstrict-read-write"/>
			<key column="Category/>
			<one-to-many class="Product"/>
		</bag>
	</class>
</hibernate-mapping>